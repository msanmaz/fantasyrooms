{
	"name": "with-tailwindcss",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"cache": "node lib/cache.js",
		"cache:clear": "node lib/cache.js --clear",
		"push": "node lib/push.js",
		"dev": "npm run cache && next",
		"build": "npm run cache && next build && npm run cache:clear",
