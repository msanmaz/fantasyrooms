		"@tailwindcss/jit": "^0.1.10",
		"build": "^0.1.4",
		"chalk": "^4.1.2",
		"concurrently": "^6.2.0",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"export": "^0.1.337",
		"framer-motion": "^4.1.17",
		"next": "^11.1.2",
		"next-build": "^0.0.1",
		"next-redux-wrapper": "^7.0.5",
		"prop-types": "^15.7.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
